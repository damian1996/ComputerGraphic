create R 4 4
fill R 0.5 0.5 0.5
put R 0 0 0 0 0
put R 1 0 0 0 0
put R 2 0 0 0 0
put R 3 0 0 0 0
put R 0 2 1 1 1
put R 1 2 1 1 1
put R 2 2 1 1 1
fft R F
get F 0 0
get F 1 0
get F 2 0
get F 3 0
get F 0 1
get F 0 2
get F 0 3
get F 1 1
get F 2 2
get F 3 3
ifft F R2
get R2 0 0
get R2 1 1
get R2 2 2
get R2 3 3
get R2 3 2
exit 0
